{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "baseUrl": "D:/Programming/Web/Fultracker",
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@layouts/*": [
